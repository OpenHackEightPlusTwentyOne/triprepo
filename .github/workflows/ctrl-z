name: crtl-z

on:
 workflow_dispatch:

jobs:
  ctrl-z:
   runs-on: ubuntu-latest
   steps:
    - name: 'Login via Azure CLI'
      uses: azure/login@v1
      with:
        creds: ${{ secrets.JB_AZURE_CREDENTIALS }}
    - name: 'make the swap'
      shell: pwsh
      run: |
       Write-Host " Ctrl-Z "
       az webapp deployment slot swap -g openhackrj195ck7rg -n openhackrj195ck7poi --slot production --target-slot staging

